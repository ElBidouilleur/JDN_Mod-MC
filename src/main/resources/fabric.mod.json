{
	"schemaVersion": 1,
	"id": "jdn_mod_mc",
	"version": "${version}",
	"name": "JDN_Mod_MC",
	"description": "This is a mod in the universe of the french minecraft book 'Journal d'un noob'. The goal of the mod is to enhance the lore and the immersion",
	"authors": [
		"PASTY and Richainer"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/PASTY667/JDN_Mod-MC"
	},
	"license": "MIT",
	"icon": "assets/jdn_mod_mc/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"journal_d_un_noob.main.JDN_ModMC"
		],
		"fabric-datagen": [
			"journal_d_un_noob.main.JDN_ModMCDataGenerator"
		],
      "client": [
            "journal_d_un_noob.main.JDN_ModMCClient"
      ]
	},
	"mixins": [
		"jdn_mod_mc.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.8",
		"java": ">=21",
		"fabric-api": "*"
